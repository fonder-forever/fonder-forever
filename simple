#include <stdio.h>
int max(int n1, int n2, int n3) {
	int result;
	if (n1>=n2&&n1>=n3) {
		result = n1;

	}
	else if (n2 >= n3 && n2 >= n1) {
		result = n2;

	}
	else { result = n3; }
		return result;
}
int main() {
	int n1, n2, n3;
	printf("inter  3 number:");
	scanf_s("%d,%d,%d", &n1, &n2, &n3);
	printf("the biggest one is : %d", max(n1,n2,n3));



}
