#include<stdio.h>

int main(){

	int n1=50;

	int n2=0;

	while (n2<=n1){

	if(n2==25){

	n2++;

	continue;

	}

	printf ("%d\n",n2);

	n2++;

	

	

	}



	return 0;}

//attention:when you use continue in while_function,you will skip all the left include increment(it is included in the statement in while_function;but in for_function it is included in the increment so it will not skip it) so you have to estate a new increment in the if_function before continue.

